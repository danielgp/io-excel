{
    "name": "danielgp/io-excel",
    "description": "Wrapper to Import from Excel into PHP Array and Export from PHP Array Into Excel using PHPExcel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":     "Daniel Popiniuc",
            "email":    "danielpopiniuc@gmail.com",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpoffice/phpspreadsheet": "*"
    },
    "autoload": {
        "psr-4": {
            "": "source/"
        },
        "classmap": [
            "source/IOExcel.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/nbproject",
            "/results",
            "/vendor"
        ]
    }
}
